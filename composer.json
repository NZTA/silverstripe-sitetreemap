{
    "name": "nzta/silverstripe-sitetreemap",
    "description": "Adds a page type that automatically builds a sitemap from the view tree. Works with Multisites module.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": ["silverstripe", "sitemap", "multisites"],
    "authors": [{
        "name": "Florian Thoma",
        "homepage": "http://www.innoweb.com.au"
    }],
    "require":
    {
        "silverstripe/cms": "^4"
    },
    "autoload": {
        "psr-4": {
            "NZTA\\Sitemap\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    }
}
